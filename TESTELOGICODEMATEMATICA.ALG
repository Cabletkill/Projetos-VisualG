Algoritmo "testelogicodematematica"
// Ricardo de Souza Silva
Var

   base, altura, largura, metrocubico : real


Inicio

  escreva("Base: ")
  Leia (base)
  escreva("ALtura: ")
  leia(altura)
  escreva("Largura: ")
  leia (largura)
  
  metrocubico<- base*altura*largura
  
  escreva("Metro Cúbico: ", metrocubico: 2:4)
  
  



Fimalgoritmo